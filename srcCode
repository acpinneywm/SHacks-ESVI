from playsound import playsound
import io
import os
import PySimpleGUI as sg
from PIL import Image

window = sg.Window(title="Hello World", layout=[[sg.Button("att ack")],[sg.Text(":)")]], margins=(500, 300))
attState = 0
playsound('theme.wav',block=False)

while True:
    event, value = window.read()
    if event == sg.WIN_CLOSED or event == 'Cancel': # if user closes window or clicks cancel
        break
    if event == 'att ack':
        window.close()
        attState = 1
        window = sg.Window(title="Hello World", layout=[[sg.Button("stop attacking")],[sg.Text("~|==> *sword stab* :(")]], margins=(500, 300))
    if event == 'stop attacking':
        window.close()
        window = sg.Window(title="Hello World", layout=[[sg.Button("att ack")],[sg.Text(":)")]], margins=(500, 300))
        attState = 0
        
window.close()
